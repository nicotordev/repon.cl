{
  "name": "repon-cl-scripts",
  "version": "1.0.0",
  "description": "Scripts for repon.cl",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup src/index.ts --format esm,cjs --dts --out-dir dist",
    "dev": "tsup src/index.ts --watch --dts --out-dir dist",
    "start": "node dist/index.js",
    "console": "node dist/index.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,md}'",
    "typecheck": "tsc --noEmit",
    "prepare": "pnpm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.29.3",
  "bin": {
    "repon-scripts": "dist/index.js"
  },
  "dependencies": {
    "@types/node": "^25.2.3",
    "@types/yargs": "^17.0.35",
    "commander": "^14.0.3",
    "eslint": "^9.39.2",
    "cheerio": "^1.0.0",
"inquirer": "^13.2.5",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "yargs": "^18.0.0"
  }
}
